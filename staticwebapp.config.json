{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/api/*", "*.{css,scss,js,png,gif,ico,jpg,svg}"]
  },
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "responseOverrides": {
    "400": {
      "rewrite": "/api/error/400"
    },
    "401": {
      "rewrite": "/api/error/401"
    },
    "403": {
      "rewrite": "/api/error/403"
    },
    "404": {
      "rewrite": "/api/error/404"
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src https: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
  },
  "mimeTypes": {
    ".json": "application/json"
  }
}